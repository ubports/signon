<aegis>
  <provide>
    <credential name="keychain-access" />
  </provide>
  <account>
    <user name="signon" group="signon"/>
  </account>
  <request>
    <credential name="UID::signon" />
    <for path="/usr/bin/signonpluginprocess" />
  </request>
  <request>
    <credential name="GID::signon" />
    <for path="/usr/bin/signonpluginprocess" />
  </request>
</aegis>
